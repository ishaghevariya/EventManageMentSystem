                                    <li><a class="hvr-sweep-to-right" href="typography.html">Short Codes</a></li>
                                </ul>
                            </li>
                            <li><a class="hvr-sweep-to-right" href="@Url.Action("Gallery","Account")">Gallery</a></li>
                            <li><a class="hvr-sweep-to-right" href="@Url.Action("Contact","Account")">Contact</a></li>
                        </ul>
                    </div>
                     @*/.navbar-collapse*@ 
                </div>
