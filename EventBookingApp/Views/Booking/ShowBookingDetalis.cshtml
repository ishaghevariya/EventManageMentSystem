@model IEnumerable<DataAcessLayer.ViewModel.BookingDetalisViewModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "List";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <br />
    <br />
    <br />
    <br />
    <br />
    <div class="card" style="background-color:ghostwhite">
        <table class="table">
            <thead>
                <tr>
                    <th style="font-weight:700;font-size:14px;color:deeppink">
                        @Html.DisplayNameFor(model => model.EquipmentType)
                    </th>
                    <th style="font-weight:700;font-size:14px;color:deeppink">
                        @Html.DisplayNameFor(model => model.FlowerType)
                    </th>
                    <th style="font-weight:700;font-size:14px;color:deeppink">
                        @Html.DisplayNameFor(model => model.FoodType)
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td style="font-weight:700;font-size:14px;color:black">
                            @Html.DisplayFor(modelItem => item.EquipmentType)
                        </td>
                        <td style="font-weight:700;font-size:14px;color:black">
                            @Html.DisplayFor(modelItem => item.FlowerType)
                        </td>
                        <td style="font-weight:700;font-size:14px;color:black">
                            @Html.DisplayFor(modelItem => item.FoodType)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        <div class="card-footer">
            <div>
                <a href='@Url.Action("Index","Booking")' class="btn btn-block" style="background-color:deeppink">Back</a>
            </div>
        </div>
    </div>
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <script src="~/plugins/js/jquery-2.2.3.min.js"></script>
    <script src="~/plugins/js/bootstrap.js"></script>
</body>
</html>