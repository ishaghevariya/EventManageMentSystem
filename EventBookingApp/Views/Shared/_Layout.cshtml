<body>
    @*<div id="placeholder"></div>*@
    <div class="header-w3layouts">
        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-fixed-top">
            <div class="container">
                <div class="navbar-header page-scroll">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                        <span class="sr-only">Events</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <h1><a class="navbar-brand" href="@Url.Action("Events","UserAccount")">Events</a></h1>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse navbar-ex1-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <!-- Hidden li included to remove active class from about link when scrolled up past about section -->
                        <li class="hidden"><a class="page-scroll" href="#page-top"></a>	</li>

                        <li><a class="hvr-sweep-to-right" href="@Url.Action("Index","UserAccount")">Home</a></li>
                        <li><a class="hvr-sweep-to-right" href="@Url.Action("About","UserAccount")">About</a></li>
                        <li><a class="hvr-sweep-to-right" href="@Url.Action("Events","UserAccount")">Events</a></li>
                        <li><a class="hvr-sweep-to-right" href="@Url.Action("Gallery","UserAccount")">Gallery</a></li>
                        <li><a class="hvr-sweep-to-right" href="@Url.Action("Contact","UserAccount")">Contact</a></li>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <li class="nav-item">
                                <a class="hvr-sweep-to-right" href="@Url.Action("InsertBooking","Booking")">BookEvent</a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle hvr-sweep-to-right" data-toggle="dropdown">Hello @User.Identity.Name <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a class="hvr-sweep-to-right" href="@Url.Action("Profile","UserAccount")">Profile</a></li>
                                    <li><a class="hvr-sweep-to-right" href="@Url.Action("UserChangePassword","UserAccount")">Change Password</a></li>
                                    <li><a class="hvr-sweep-to-right" href="@Url.Action("LogOut","UserAccount")">LogOut</a></li>
                                </ul>
                            </li>
                        }
                        else
                        {
                            <li>
                                <a class="hvr-sweep-to-right" href="@Url.Action("Login","UserAccount")">SignIn</a>
                            </li>
                        }
                    </ul>
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container -->
        </nav>
    </div>
    <div>@RenderBody()</div>
    <div class="footer-top">
        <div class="container">
            <div class="col-md-3 foot-left">
                <h3>About Us</h3>

                <p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard.</p>
            </div>
